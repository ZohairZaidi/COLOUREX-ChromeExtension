{
    "name": "COLOUREX",
    "description": "Hex Colour Coder Retriver",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/logo/16.png",
        "32": "/logo/32.png",
        "48": "/logo/48.png"
    }
}